{
  "name": "fordva_server",
  "version": "0.1.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "build": "sucrase src/ -d build/ --transforms imports",
    "start": "node build/server.js",
    "dev:start": "nodemon src/server.js",
    "dev:queue": "nodemon src/queue.js",
    "db:run": "npm run migration:run && npm run seed:run",
    "migration:run": "sequelize db:migrate",
    "seed:run": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo:all",
    "seed:file": "sucrase-node src/importCsv.js",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint \"src/**/*.js\" --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.6.0",
    "cors": "^2.8.5",
    "csv-parser": "^2.3.3",
    "dotenv": "^8.2.0",
    "expo-server-sdk": "^3.6.0",
    "express": "^4.17.1",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "prettier": "^2.1.1",
    "sequelize": "^5.21.6",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "sequelize-cli": "^6.2.0",
    "sucrase": "^3.15.0"
  }
}
